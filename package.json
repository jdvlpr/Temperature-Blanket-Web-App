{
  "name": "temperature-blanket-web-app",
  "version": "5.39.3",
  "description": "Weather Data + Art! Web app for creating a temperature blanket",
  "main": "index.html",
  "private": true,
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --plugin-search-dir . --check . && eslint .",
    "format": "prettier --plugin-search-dir . --write .",
    "test": "pnpm test:unit && pnpm test:integration && pnpm test:e2e",
    "test:unit": "vitest run src",
    "test:integration": "start-server-and-test 'vite dev --host localhost --port 5180' http://localhost:5180 'vitest run tests/integration'",
    "test:e2e": "playwright test"
  },
  "dependencies": {
    "@floating-ui/dom": "^1.7.4",
    "@lucide/lab": "^0.1.2",
    "@lucide/svelte": "^0.562.0",
    "autocompleter": "^9.3.2",
    "canvg": "^4.0.3",
    "chart.js": "^4.5.1",
    "chroma-js": "^3.2.0",
    "getimagepalette": "^2.4.0",
    "idb-keyval": "^6.2.2",
    "jspdf": "^4.0.0",
    "node-fetch": "^3.3.2",
    "suncalc": "^1.9.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.57.0",
    "@skeletonlabs/skeleton": "^4.10.0",
    "@skeletonlabs/skeleton-svelte": "^4.10.0",
    "@sveltejs/adapter-cloudflare": "^7.2.5",
    "@sveltejs/kit": "^2.49.5",
    "@sveltejs/vite-plugin-svelte": "^6.2.4",
    "@tailwindcss/forms": "^0.5.11",
    "@tailwindcss/vite": "^4.1.18",
    "@typescript-eslint/eslint-plugin": "^8.53.0",
    "@typescript-eslint/parser": "^8.53.0",
    "@vincjo/datatables": "^2.8.0",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "prettier": "^3.8.0",
    "prettier-plugin-svelte": "^3.4.1",
    "prettier-plugin-tailwindcss": "^0.7.2",
    "start-server-and-test": "^2.1.3",
    "svelte": "^5.46.4",
    "svelte-check": "^4.3.5",
    "svelte-dnd-action": "^0.9.69",
    "tailwindcss": "^4.1.18",
    "tslib": "^2.8.1",
    "typescript": "^5.9.3",
    "vaul-svelte": "1.0.0-next.3",
    "vite": "^7.3.1",
    "vitest": "^4.0.17"
  },
  "browserslist": "> 0.25%",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/jdvlpr/Temperature-Blanket-Web-App.git"
  },
  "author": "",
  "license": "LGPL-3.0-or-later",
  "homepage": "https://github.com/jdvlpr/Temperature-Blanket-Web-App.git",
  "pnpm": {
    "onlyBuiltDependencies": [
      "@parcel/watcher",
      "@tailwindcss/oxide",
      "core-js",
      "esbuild",
      "sharp",
      "workerd"
    ]
  }
}
